{"sections":[],"abstract":[{"type":"text","text":"The notify signal is emitted on an object when one of its properties has"},{"type":"text","text":" "},{"type":"text","text":"its value set through "},{"type":"codeVoice","code":"g_object_set_property()"},{"text":", ","type":"text"},{"code":"g_object_set()","type":"codeVoice"},{"type":"text","text":", et al."}],"primaryContentSections":[{"declarations":[{"tokens":[{"text":"@discardableResult","kind":"attribute"},{"kind":"text","text":" "},{"text":"func","kind":"keyword"},{"text":" ","kind":"text"},{"text":"onNotify","kind":"identifier"},{"kind":"text","text":"("},{"kind":"externalParam","text":"flags"},{"kind":"text","text":": "},{"identifier":"doc:\/\/GLibObject\/documentation\/GLibObject\/ConnectFlags","text":"ConnectFlags","preciseIdentifier":"s:10GLibObject12ConnectFlagsV","kind":"typeIdentifier"},{"kind":"text","text":" = ConnectFlags(0), "},{"text":"handler","kind":"externalParam"},{"text":": ","kind":"text"},{"kind":"keyword","text":"@escaping"},{"kind":"text","text":" ("},{"identifier":"doc:\/\/GLibObject\/documentation\/GLibObject\/ObjectRef","kind":"typeIdentifier","text":"ObjectRef","preciseIdentifier":"s:10GLibObject0B3RefV"},{"text":", ","kind":"text"},{"kind":"typeIdentifier","identifier":"doc:\/\/GLibObject\/documentation\/GLibObject\/ParamSpecRef","text":"ParamSpecRef","preciseIdentifier":"s:10GLibObject12ParamSpecRefV"},{"kind":"text","text":") -> "},{"kind":"typeIdentifier","preciseIdentifier":"s:s4Voida","text":"Void"},{"kind":"text","text":") -> "},{"kind":"typeIdentifier","text":"Int","preciseIdentifier":"s:Si"}],"platforms":["macOS"],"languages":["swift"]}],"kind":"declarations"},{"parameters":[{"content":[{"inlineContent":[{"type":"text","text":"Flags"}],"type":"paragraph"}],"name":"flags"},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The signal handler to call"},{"type":"text","text":" "},{"text":"Run the given callback whenever the ","type":"text"},{"code":"notify","